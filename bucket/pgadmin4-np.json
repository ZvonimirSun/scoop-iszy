{
    "homepage": "https://www.pgadmin.org",
    "description": "pgAdmin is the most popular and feature rich Open Source administration and development platform for PostgreSQL.",
    "version": "4.12",
    "url": "https://ftp.postgresql.org/pub/pgadmin/pgadmin4/v4.12/windows/pgadmin4-4.12-x86.exe",
    "hash": "09981cc9a3d050fb8c1ff5e0ae45012e9882360f2a4056f2b1b79c9136ec1a7a",
    "bin": [
        "runtime\\psql.exe",
        "runtime\\pgAdmin4.exe",
        "runtime\\pg_restore.exe",
        "runtime\\pg_dumpall.exe",
        "runtime\\pg_dump.exe"
    ],
    "installer": {
        "args": [
            "/verysilent",
            "/nocancel",
            "/norestart",
            "/dir=$dir"
        ]
    },
    "uninstaller": {
        "file": "unins000.exe",
        "args": [
            "/verysilent",
            "/nocancel",
            "/norestart"
        ]
    },
    "checkver": {
        "url": "https://www.pgadmin.org/download/pgadmin-4-windows/",
        "re": "pgAdmin 4 v([\\d.]+)"
    },
    "autoupdate": {
        "url": "https://ftp.postgresql.org/pub/pgadmin/pgadmin4/v$version/windows/pgadmin4-$version-x86.exe"
    }
}
